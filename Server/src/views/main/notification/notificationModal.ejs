<div class="modal fade" id="notificationsModal" role="dialog">
  <div class="modal-dialog modal-lg">
      <div class="modal-content">
          <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal">&times;</button>
              <h4 class="modal-title">Tất cả thông báo</h4>
          </div>
          <div class="modal-body">
              <div class="mark-notif-as-read">
                 <a id="modal-mark-notif-as-read" href="javascript:void(0)">
                    <strong>Đánh dấu tất cả là đã đọc</strong>
                 </a>
                </div>
                <hr>
              <ul class="list-notifications">
                <%notifications.forEach(function(notification){ %>

                  
                    <li>
                        <%-notification%>
                    </li>

             <% })%>
                 
               
              </ul>
              <hr>
              <div class="read-more-notif"> 
                  <a id="link-read-more-notif" href="javascript:void(0)">
                <strong>Xem thêm 10 thông báo</strong>
             </a>
             <div class="read-more-notif-loader"><div></div><div></div></div>

            </div>
          </div>
      </div>
  </div>
</div>
  